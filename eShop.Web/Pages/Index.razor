@page "/"
@inject ISearchProductUseCase searchProduct


<h1>Hello, world!</h1>

@foreach (var item in products)
{
    <text>Brand:</text>@item.Brand
    <br />
}

@code{
    private IEnumerable<Product> products;

    protected override void OnInitialized()
    {
        base.OnInitialized();
        products = searchProduct.Execute();
    }
}